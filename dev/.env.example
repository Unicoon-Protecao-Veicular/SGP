# Versão do Camunda
CAMUNDA_VERSION=latest

# Porta do ambiente
DEV_PORT=8080

# Configurações do banco de dados
DB_DRIVER=org.postgresql.Driver
DB_NAME=camunda
DB_URL=jdbc:postgresql://postgres-dev:5432/camunda
DB_USERNAME=camunda
DB_PASSWORD=devpassword456

# Credenciais de administração
CAMUNDA_ADMIN_USER=admin-dev
CAMUNDA_ADMIN_PASSWORD=devpassword123

# -----------------------------
# Camunda Web Modeler (DEV)
# -----------------------------

# Versão das imagens do Web Modeler
MODELER_VERSION=latest

# Base URL pública do Web Modeler (UI)
MODELER_SERVER_URL=http://localhost:8083

# Banco do Web Modeler
MODELER_DB_VERSION=15
MODELER_DB_NAME=modeler
MODELER_DB_USER=modeler
MODELER_DB_PASSWORD=modeler

# WebSockets (Pusher) - chaves de desenvolvimento
PUSHER_APP_ID=web-modeler
PUSHER_APP_KEY=dev-key
PUSHER_APP_SECRET=dev-secret
PUSHER_APP_PATH=/

# OIDC / Identity / Keycloak
KEYCLOAK_REALM=camunda-platform
OAUTH2_CLIENT_ID=web-modeler

# SMTP (emails do Modeler REST API)
RESTAPI_MAIL_FROM_ADDRESS=noreply@example.com
RESTAPI_MAIL_FROM_NAME=Camunda